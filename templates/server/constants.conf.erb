/* managed by puppet */

<%= @plugindir ? "const PluginDir = \"#{plugindir}\"" : "// const PluginDir =" %>

<%= @manubulonplugindir ? "const ManubulonPluginDir = \"#{manubulonplugindir}\"" : "// const ManubulonPluginDir =" %>

<%= @plugincontribdir ? "const PluginContribDir = \"#{plugincontribdir}\"" : "// const PluginContribDir =" %>

<% if @zonename -%>
<%  if scope.function_is_fqdn([@zonename]) -%>
const ZoneName = "<%= @zonename %>"
<%  else -%>
const ZoneName = <%= @zonename %>
<%  end -%>
<% end -%>

<%= @nodename ? "const NodeName = \"#{nodename}\"" : "// const NodeName =" %>

<%= @ticketsalt ? "const TicketSalt = \"#{ticketsalt}\"" : "// const TicketSalt =" %>


