/* managed by puppet */

include <itl>
include <plugins>
<% if @include_files -%>
<% include_files.each do |include_file| -%>
include "<%= include_file %>"
<% end -%>
<% end -%>

<% if @include_directories -%>
<% include_directories.each do |include_dir| -%>
include_recursive "<%= include_dir %>"
<% end -%>
<% end -%>


