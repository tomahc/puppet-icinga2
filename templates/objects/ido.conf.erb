/* managed by puppet */

library "<%= @library %>"

object <%= @object ? "#{object}" : 'IdoMysqlConnection' %> "ido-mysql" {

  <%= @user ? "user = \"#{user}\"" : '// user =' -%>,

  <%= @password ? "password = \"#{password}\"" : '// password =' -%>,

  <%= @host ? "host = \"#{host}\"" : '// host =' -%>

  <%= @database ? "database = \"#{database}\"" : '// database =' -%>

}


