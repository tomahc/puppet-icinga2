/* managed by puppet */

<%= @is_template ? "template" : "object" %> TimePeriod "<%= @name %>" {
<% if ! @is_template -%>
  display_name = "<%= @display_name %>"
<% end -%>

<% if @templates -%>
<% @templates.each do |template| -%>
  import "<%= template %>"
<% end -%>
<% end -%>

  <%= @update ? "update = #{update}" : '' -%>

<% if @ranges -%>
  ranges = {
<%  ranges.each do |var,val| -%>
    "<%= var %>" = "<%= val %>",
<%  end -%>
  }
<% end -%>

}


